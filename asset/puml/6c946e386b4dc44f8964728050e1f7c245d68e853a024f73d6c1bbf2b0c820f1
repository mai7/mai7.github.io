@startuml
class Client

class Promisor {
	+Promise compute()
}
class TaskExecutor {
	+run()
}
class Result

class Promise {
	+Result getResult()
	+setResult( Result )
	+isDone()
}

Client ..> Promisor

Client ..> Result

Promisor ..> TaskExecutor

TaskExecutor ..> Promise 

TaskExecutor ..> Result


@enduml